{
    "coc.preferences.extensionUpdateCheck": "daily",
    "coc.preferences.jumpCommand": "tab drop",
    "coc.preferences.formatOnType": false,
    "coc.preferences.currentFunctionSymbolAutoUpdate": true,
    "codeLens.enable": true,
    "diagnostic.errorSign": "‚úò",
    "diagnostic.virtualText": false,
    "diagnostic.virtualTextPrefix": " ‚ùØ‚ùØ‚ùØ ",
    "outline.autoPreview": true,
    "outline.expandLevel": 0,
    "suggest.triggerAfterInsertEnter": true,
    "suggest.timeout": 5000,
    "suggest.noselect": true,
    "suggest.minTriggerInputLength": 2,
    "suggest.detailField": "abbr",
    "suggest.snippetIndicator": "üåü",
    "suggest.triggerCompletionWait": 50,
    "pyright.inlayHints.functionReturnTypes": true,
    "pyright.inlayHints.variableTypes": true,
    "pyright.inlayHints.parameterTypes": false,
    "python.formatting.provider": "yapf",
    "python.formatting.yapfPath": "~/bin/yapf",
    "python.formatting.yapfArgs": ["--style={based_on_style: pep8, column_limit: 120}"],
    "python.linting.enabled": true,
    "python.linting.ruffEnabled": true,
    "python.linting.ruffPath": "~/bin/ruff",
    "prettier.printWidth": 120,
    "javascript.format.enable": false,
    "typescript.format.enable": false
}